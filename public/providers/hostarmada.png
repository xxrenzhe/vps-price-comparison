/* Tab Switcher */
.tab-switcher .tabs-wrapper {
	position: sticky;
	top: 75px;
	background: #f7fbfc;
	z-index: 999;
	display: flex;
	grid-gap: 10px;
	align-items: flex-start;
	margin: 0 -40px;
	padding: 0 40px;
	margin-bottom: 20px;
}

.tab-switcher .tab-lable {
	flex: 0 0 40px;
	font-size: 16px;
	line-height: 26px;
	margin-right: 10px;
	position: relative;
	top: 20px;
}

.tab-switcher .tabs {
	white-space: nowrap;
	overflow: scroll auto;
	-webkit-overflow-scrolling: touch;
	-ms-overflow-style: -ms-autohiding-scrollbar;
	-webkit-user-select: none;
	-ms-user-select: none;
	user-select: none;
	text-align: center;
	padding: 15px 0 10px;
	margin-bottom: 15px;
}

.tab-switcher .tabs button {
	background: none;
	font-size: 15px;
	line-height: 15px;
	padding: 8px 18px;
	border-radius: 5px;
	transition: 0.2s ease-in-out;
	color: var(--text);
	border: 1px solid #ddd;
	background:#fff;
}

.tab-switcher .tabs button:hover {
	background: var(--primary);
	color: #fff;
}

.tab-switcher .tabs button.active {
	background: var(--primary);
	color: #fff;
	border: 1px solid var(--primary);
}

.tab-switcher .tab-content {
	display: none;
}

.tab-switcher .tab-content.active {
	display: block;
	background: none;
}

/* scrollbar */
.tab-switcher .tabs::-webkit-scrollbar {
	height: 4px;

}

.tab-switcher .tabs::-webkit-scrollbar-thumb {
	background: #eee;
	border-radius: 50px;
}


@media (max-width: 768px) {


	/* Tab Switcher */
	.tab-switcher .tabs-wrapper {
		top: 60px;
		margin: 0 -20px;
		padding: 0 20px;
		margin-bottom: 10px;
	}

	.tab-switcher .tab-lable {
		font-size: 14px;
		line-height: 20px;
		top: 25px;
		margin-right: 0;
	}

	.tab-switcher .tabs {
		padding: 20px 0 10px;
		margin-bottom: 15px;
	}

	.tab-switcher .tabs button {
		font-size: 14px;
		line-height: 14px;
		padding: 6px 15px;
	}

	.tab-switcher .tab-content {
		display: none;
	}
}

